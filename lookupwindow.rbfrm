ry			dim p as picture = newPicture(self.width,self.height,32)			dim g as graphics = p.graphics			dim printG as graphics = openprinterdialog()			if g<> nil then			self.drawinto g,1,1			printG.drawPicture p,1,1			end if			catch			end try		End Function	#tag EndMenuHandler	#tag Method, Flags = &h0		Sub loadinfo()		  // load anime infomation		  dim data as string		  socket1.setrequestheader "Authorization","Basic " + app.loginenc		  data= socket1.get("http://mal-api.com/anime/" + SeriesID + "?format=xml&mine=1" ,10)		  If data is nil then		    app.errboxshow("There is a problem looking up infomation!", "Cannot retrieve infomation. Make sure your internet connection is working.")		  else		    Dim XML As New XmlDocument		    XML.LoadXML(Data)		    Dim AppNode As XmlNode = XML.DocumentElement		    Dim CNode As XmlNode = AppNode.firstchild		    dim epi as string		    dim t1 as string		    dim t2 as string		    // Reset Genre/Alt Titles		    Genre.text = "Genre:"		    alttitle.text = "Also known as:"		    //load data		    While CNode <> Nil		      Select Case CNode.Name		      Case "Title"		        me.TrueWindow.title = CNode.FirstChild.Value + " - Anime Information"		        seriestitle.text = CNode.FirstChild.Value		      Case "Status"		        Status.text = "Status: " + CNode.FirstChild.Value		      Case "Synopsis"		        synopsistext.text = replaceall(CNode.FirstChild.Value, "<br>",  "" + EndOfLine  + "" )		      Case "Episodes"		        epi = cnode.firstchild.value		        If epi = "0" then		          Episodes.text = "Episodes: Unknown"		        else		          Episodes.text = "Episodes: " + epi		        end if		      Case "Rank"		        Ranking.text = "Ranked: " + CNode.FirstChild.Value		      Case "members_score"		        Score.text = "Score: " + CNode.FirstChild.Value + "/10"		      Case "Type"		        Type.text = "Type: " + CNode.FirstChild.Value		      Case "Classification"		        classification.text = "Classification: " + CNode.FirstChild.Value		      Case "Genre"		        Genre.text = Genre.text + " " +  CNode.child(0).Value + ", "		      Case "popularity_rank"		        Popularity.text = "Popularity: " + CNode.FirstChild.Value		      Case "favorited_count"		        Favorites.text = "Favorites: " + CNode.FirstChild.Value		      Case  "members_count"		        members.text = "Members: " + CNode.FirstChild.Value		      Case  "synonym"		        t1 = CNode.FirstChild.Value		      Case  "english_title"		        t2 = CNode.FirstChild.Value		      Case "image_url"		        dim s as picture		        // Obtain the image		        s = getpictFromURL(CNode.FirstChild.Value)		        // Resizing Image to fit Canvas (aniimage)		        Dim p as New Picture(aniimage.Width,aniimage.Height,32) //creating new picture (width, height, depth)		        		        p.Graphics.DrawPicture (s,0,0,aniimage.width, aniimage.height,0,0,s.width, s.height)		        		        aniimage.Backdrop = p // loading the picture from memory to Aniipicture		        Aniimage.Refresh		        p = nil		        s= nil		      Case "watched_status"		        If CNode.FirstChild is nil then		          //Hide yourstatus		          yourstatus.visible = false		          lookuptoolbar1.AddList.enabled = true		          lookuptoolbar1.updateinfo.enabled = false		          DelSeries.visible = false		          notonlist.visible = true		        else		          watchstatus.text = "Status: " + cnode.firstchild.value		          yourstatus.visible = true		          lookuptoolbar1.AddList.enabled = false		          lookuptoolbar1.updateinfo.enabled = true		          DelSeries.visible = true		          notonlist.visible = false		        end if		      Case "watched_episodes"		        If yourstatus.visible = false then		        else		          epiwatch = cnode.firstchild.value		          If epi = "0" then		            episodeswatched.text = "Episodes: " + epiwatch		          else		            episodeswatched.text = "Episodes: " + epiwatch+ "/" + epi		          end if		        end if		      case "score"		        If cnode.firstchild is nil then		        else		          yourscore.text = "Score: " + cnode.firstchild.value + "/10"		        end if		      End Select		      CNode = CNode.NextSibling		    Wend		    if t1 = "" then		      alttitle.text = alttitle.text + " " + t2		    else		      alttitle.text = alttitle.text + " " + t1 + ", " + t2		    end if		    		    // release properties		    xml = nil		    appnode = nil		    cnode = nil		    data = ""		    t1= ""		    t2 = ""		  end if		End Sub	#tag EndMethod	#tag Method, Flags = &h0		Sub loadpopupmenu()		  //Lookup Menu		  Dim lookupmenu as New MenuItem		  Dim MAL as New MenuItem		  Dim AniDB as New MenuItem		  Dim Tokyosho as New MenuItem		  Dim Melative as New MenuItem		  		  // Append Menu Text		  // Lookup Menu		  MAL.Text="Open MAL Page"		  AniDB.Text="Find at AniDB"		  Tokyosho.text="Find at TokyoTosho"		  Melative.text = "Find at Melative"		  		  // Append Menu		  // lookup menu		  lookupmenu.Append MAL		  lookupmenu.Append AniDB		  lookupmenu.Append Tokyosho		  lookupmenu.Append Melative		  		  //Apply Menu to the toolbaritem		  lookuptoolbar1.open.DropdownMenu = lookupmenu		End Sub	#tag EndMethod	#tag Property, Flags = &h0		epiwatch As string	#tag EndProperty	#tag Property, Flags = &h0		seriesid As string	#tag EndProperty#tag EndWindowCode#tag Events lookuptoolbar1	#tag Event		Sub Action(item As ToolItem)		  Select Case Item.Name		  case "addlist"		    dim w as new updatewindow		    w.aniid = seriesid		    w.upmethod = "Add"		    w.seriestitle.text = "Adding: "+  seriestitle.text		    If prefs.BooleanMelUpdates = true then		      w.titleprob = seriestitle.text		    end if		    w.addloadinfo()		    w.ShowModalWithin(self.TrueWindow)		    if w.doIt = true then		      loadinfo		    end if		    w.close		  Case "updateinfo"		    dim w as new updatewindow		    w.aniid = seriesid		    w.upmethod = "Update"		    w.seriestitle.text = "Updating Information for: "+  seriestitle.text		    If prefs.BooleanMelUpdates = true then		      w.titleprob = seriestitle.text		    end if		    w.loadinfo()		    w.ShowModalWithin(self.TrueWindow)		    if w.doIt = true then		      loadinfo		    end if		    w.close		  case "melupdatebut"		    If prefs.BooleanMelUpdates = true then		      dim mu as new melupdatewindow		      mu.titletext.Text = seriestitle.text		      mu.truewindow.Title = "Post Melative Update - " + seriestitle.text		      if yourstatus.visible = false then		      else		        mu.epi.text= epiwatch		        mu.checkbox1.value = true		      end if		      mu.Show		    else		      app.errboxshow("MAL Client OS X was unable to post a melative update on this series.","You must configure your melative credentials before you can post a melative update on this series. Configure your credentials, enable melative updating and try again.")		    end if		  case "email"		    showurl "mailto:?subject=Anime Suggestion - " + seriestitle.caption + "&body=" + seriestitle.caption + " - http://myanimelist.net/anime.php?id=" + seriesid		  End Select		End Sub	#tag EndEvent	#tag Event		Sub DropDownMenuAction(item As ToolItem, hitItem As MenuItem)		  select case hititem.Text		    		  case "Open MAL Page"		    ShowURL "http://myanimelist.net/anime.php?id=" + seriesid		  case "Find at AniDB"		    showURL "http://anidb.net/perl-bin/animedb.pl?show=animelist&adb.search=" + EncodeURLComponent(SeriesTitle.caption) + "&do.search=search"		  case "Find at TokyoTosho"		    showURL "http://www.tokyotosho.info/search.php?terms=" + EncodeURLComponent(SeriesTitle.caption) + "&type=0"		  case "Find at Melative"		    showURL "http://melative.com/anime/" + EncodeURLComponent(SeriesTitle.caption)		  end select		End Sub	#tag EndEvent#tag EndEvents#tag Events DelSeries	#tag Event		Sub Action()		  		  Dim d as New MessageDialog //declare the MessageDialog object		  Dim b as MessageDialogButton //for handling the result		  d.icon=MessageDialog.GraphicCaution //display warning icon		  d.ActionButton.Caption="Delete"		  d.CancelButton.Visible=True    //show the Cancel button		  d.Message="Are you sure you want to delete " + SeriesTitle.text + " from your list?"		  d.Explanation="Once deleted, this operation cannot be undone."		  b=d.ShowModalWithin(self.TrueWindow) //display the dialog		  Select Case b //determine which button was pressed.		  Case d.ActionButton		    //user pressed Delete		    //perform delete		    dim form as dictionary		    form = New Dictionary		    form.value("_method") = "DELETE"		    // setup the socket to POST the form		    socket1.setFormData form		    socket1.setrequestheader "Authorization","Basic " + app.loginenc		    socket1.post ("http://mal-api.com/animelist/anime/" + SeriesID )		    form = nil		    If socket1.httpstatuscode = 200 then		      Msgbox SeriesTitle.text + " is removed from your list."		      window1.refreshlist(true)		      loadinfo		    else		      app.errboxshow("There was a problem when we were trying to remove a title.", "Check your internet connection and try removing that title again.")		    end if		  Case d.CancelButton		    //user pressed Cancel		  End select		  		End Sub	#tag EndEvent#tag EndEvents