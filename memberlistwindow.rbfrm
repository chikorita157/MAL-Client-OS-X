 = parseAnime(p.Item(i))		      List.AddRow(d.Value("id"))		      List.Cell(List.LastIndex, 1) = d.Value("title")		      If d.Value("episodes") = 0 then		        List.Cell(List.LastIndex, 2) = d.Value("watched_episodes") + "/" + "-"		      else		        List.Cell(List.LastIndex, 2) = d.Value("watched_episodes") + "/" + d.Value("episodes")		      end if		      List.Cell(List.LastIndex, 3) = d.Value("type")		      List.Cell(List.LastIndex, 4) = d.Value("score")		      List.Cell(List.LastIndex, 5) = d.Value("watched_status")		    next		    list.visible = true		    xml = nil		    r = nil		    d = nil		    c = 0		    list.listboxsort(5)		    statustext.visible = false		    ProgressWheel1.visible = false		  else		    self.errboxshow("MAL Client OS X was unable to retrieve the user's Anime list.",  "Make sure you are connected to the internet or you have specified the correct username.")		    ProgressWheel1.visible = false		    statustext.visible = false		  end if		  		  		  		End Sub	#tag EndMethod	#tag Property, Flags = &h0		c As Integer	#tag EndProperty	#tag Property, Flags = &h0		d As dictionary	#tag EndProperty	#tag Property, Flags = &h0		i As Integer	#tag EndProperty	#tag Property, Flags = &h0		p As XmlNodeList	#tag EndProperty	#tag Property, Flags = &h0		r As XmlNode	#tag EndProperty#tag EndWindowCode#tag Events TextField1	#tag Event		Function KeyDown(Key As String) As Boolean		  if key = chr(13) then		    getlist(me.text)		  end if		End Function	#tag EndEvent#tag EndEvents#tag Events PushButton1	#tag Event		Sub Action()		  getlist(textfield1.text)		End Sub	#tag EndEvent#tag EndEvents#tag Events List	#tag Event		Function CellBackgroundPaint(g As Graphics, row As Integer, column As Integer) As Boolean		  if row mod 2 = 0 then		    g.forecolor = GetThemeColor(kThemeBrushListViewOddRowBackground)		  else		    g.forecolor = GetThemeColor(kThemeBrushListViewEvenRowBackground)		  end		  g.fillrect(0,0,g.width,g.height)		End Function	#tag EndEvent	#tag Event		Function KeyDown(Key As String) As Boolean		  if key = chr(13) then		    app.lookupwindowshow( me.cell(me.listindex,0))		  end if		End Function	#tag EndEvent	#tag Event		Sub DoubleClick()		  app.lookupwindowshow( me.cell(me.listindex,0))		End Sub	#tag EndEvent#tag EndEvents